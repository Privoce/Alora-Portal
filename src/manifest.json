{
  "manifest_version": 2,
  "name": "Portal - My Personal Front Page",
  "version": "1.0.0",
  "description": "",
  "permissions": [
    "tabs",
    "storage",
    "history",
    "chrome://favicon/",
    "https://*/"
  ],
  "content_security_policy": "img-src chrome://favicon;",
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  }
}
