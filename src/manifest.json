{
  "manifest_version": 2,
  "name": "Tab Manager",
  "version": "1.1",
  "description": "Your smart working assistant!",
  "permissions": [
    "tabs",
    "storage",
    "history",
    "chrome://favicon/"
  ],
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  }
}