{
  "manifest_version": 2,
  "name": "Alora Portal",
  "version": "1.0.0",
  "description": "",
  "permissions": [
    "tabs",
    "storage",
    "history",
    "chrome://favicon/",
    "*://papo.privoce.com/*",
    "*://auth.privoce.com/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; img-src chrome://favicon;",
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  }
}
